class Market:
    def __init__(self):
        self.queue = []

    # QueueBehaviour methods
    def enqueue(self, person):
        self.queue.append(person)
    
    def dequeue(self):
        if self.queue:
            return self.queue.pop(0)
        else:
            return None

    def get_queue_length(self):
        return len(self.queue)

